<%- model_class = Schedule -%>
<div class="page-header">
  <h1><%=t '.title', :default => model_class.model_name.human.pluralize.titleize %></h1>
</div>
<table class="table table-striped" id="schedules">
  <thead>
    <tr>
      <th><%= model_class.human_attribute_name(:id) %></th>
      <th><%= model_class.human_attribute_name(:backlog_id) %></th>
      <th><%= model_class.human_attribute_name(:sono) %></th>
      <th><%= model_class.human_attribute_name(:productCode) %></th>
      <th><%= model_class.human_attribute_name(:deliveryDate) %></th>
      <th><%= model_class.human_attribute_name(:priority) %></th>
      <th><%= model_class.human_attribute_name(:amt) %></th>
      <th><%= model_class.human_attribute_name(:seq) %></th>
      <th><%= model_class.human_attribute_name(:scheduler) %></th>
      <th><%=t '.actions', :default => t("helpers.actions") %></th>
    </tr>
  </thead>
  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= link_to schedule.id, schedule_path(schedule) %></td>
        <td><%= schedule.backlog_id %></td>
        <td><%= schedule.sono %></td>
        <td><%= schedule.productCode %></td>
        <td><%= schedule.deliveryDate %></td>
        <td><%= schedule.priority %></td>
        <td><%= schedule.amt %></td>
        <td><%= schedule.seq %></td>
        <td><%= schedule.scheduler %></td>
        <td>
          <%= link_to t('.edit', :default => t("helpers.links.edit")),
                      edit_schedule_path(schedule), :class => 'btn btn-default btn-xs' %>
          <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
                      schedule_path(schedule),
                      :method => :delete,
                      :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
                      :class => 'btn btn-xs btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to t('.new', :default => t("helpers.links.new")),
            new_schedule_path,
            :class => 'btn btn-primary' %>
